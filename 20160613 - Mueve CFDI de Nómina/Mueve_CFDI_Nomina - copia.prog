bye
EOF
ftp -n 192.1.1.191 << EOF
verbose off
prompt off
passive off
user developer oracle
cd /var/tmp/CARGAS/NOMINA/CFDI_NOMINA/RESPALDO_CFDI_NOMINA
bin
put $param1
bye
EOF
rm $param1