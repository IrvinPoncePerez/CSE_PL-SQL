#!/bin/bash
source_dir="$5"
file_name="$6"
out_file_name="$7"
pdf_file_name="$8"
xml_extension="$9"
pdf_extension="${10}"
pwd
echo $source_dir
echo $file_name
echo $out_file_name
echo $pdf_file_name
echo $xml_extension
echo $pdf_extension
cd $source_dir
pwd
cp $file_name"."$xml_extension $out_file_name
cp $file_name"."$pdf_extension $pdf_file_name
rm $file_name"."$xml_extension
rm $file_name"."$pdf_extension