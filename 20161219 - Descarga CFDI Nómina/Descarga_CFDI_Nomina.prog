bin
mget *.xml
cd $param1/$param5
bin
mput *.xml
cd $param1
mdelete *.xml
bin
mget *.pdf
cd $param1/$param5
bin
mput *.pdf
cd $param1
mdelete *.pdf
bye
EOF
rm *.xml
rm *.pdf